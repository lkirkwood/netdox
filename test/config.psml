        <section fragmenttype="title" id="title">
                <fragment xmlns:t="http://pageseeder.com/psml/template" id="title" type="title">
            
                        <heading level="1">Config</heading>
            
                        <para>In this document you may configure attributes associated with a document label.
                                In order to apply those attributes to a document, simply label it with the specified label.</para>
        
                </fragment>
        </section>
        <section id="subnets" fragmenttype="subnet">
                <fragment id="subnets_title" type="subnet">
                        <heading level="2">Subnets</heading>
                        <preformat>Define any public IPv4 subnets you control here.
                                The location property will be inherited by NetworkObjects using an IP in the subnet.</preformat>
                </fragment>
                <properties-fragment id="2" type="subnet">

                        <property name="subnet" title="Subnet" value="192.168.6.0/24"/>

                        <property name="location" title="Location" value="SY4"/>

                </properties-fragment>
                <properties-fragment id="4" type="subnet">

                        <property name="subnet" title="Subnet" value="192.168.7.0/24"/>

                        <property name="location" title="Location" value="SY4"/>

                </properties-fragment>
                <properties-fragment id="6" type="subnet">

                        <property name="subnet" title="Subnet" value="192.168.13.0/24"/>

                        <property name="location" title="Location" value="SY4"/>

                </properties-fragment>
                <properties-fragment id="7" type="subnet">

                        <property name="subnet" title="Subnet" value="192.168.0.0/16"/>

                        <property name="location" title="Location" value="PYR"/>

                </properties-fragment>
                <properties-fragment id="8" type="subnet">

                        <property name="subnet" title="Subnet" value="10.0.0.0/8"/>

                        <property name="location" title="Location" value="AWS"/>

                </properties-fragment>
        </section>
        <section fragmenttype="label" id="labels">
                <fragment xmlns:t="http://pageseeder.com/psml/template" id="labels_title" type="label">
            
                        <heading level="2">Labels</heading>
            
                        <preformat>Each label may have an arbitrary map of attributes associated with it, which will be inherited by NetworkObjects bearing the label.
                                To configure a label, create a new fragment in this section and populate the properties.</preformat>
        
                </fragment>
                <properties-fragment id="label_website-no-ssl">

                        <property name="label" value="website-no-ssl"/>

                        <property name="screenshot" value="yes"/>

                        <property name="icinga_template" value="website-no-ssl"/>

                        <property name="ssl" value="no"/>

                </properties-fragment>
                <properties-fragment id="label_website-ssl-auth">

                        <property name="label" value="website-ssl-auth"/>

                        <property name="screenshot" value="no"/>

                        <property name="icinga_template" value="website-ssl-auth"/>

                        <property name="ssl" value="yes"/>

                </properties-fragment>
                <properties-fragment id="label_website-ssl-only">

                        <property name="label" value="website-ssl-only"/>

                        <property name="screenshot" value="yes"/>

                        <property name="icinga_template" value="website-ssl-only"/>

                        <property name="ssl" value="yes"/>

                </properties-fragment>
                <properties-fragment id="label_unmonitored">

                        <property name="label" value="unmonitored"/>

                        <property name="screenshot" value="no"/>

                        <property name="icinga_template" value="None"/>

                        <property name="ssl" value="no"/>

                </properties-fragment>
                <properties-fragment id="label_netdox-default">

                        <property name="label" value="netdox-default"/>

                        <property name="screenshot" value="no"/>

                        <property name="icinga_template" value="generic-host"/>

                        <property name="ssl" value="no"/>

                </properties-fragment>
        </section>
        <section fragmenttype="exclusions" id="exclusions">
                <fragment id="exclude" type="exclusions">
                        
                        <heading level="2">Exclusions</heading>
                        
                        <preformat>
                                These domains will be excluded from the network entirely. Netdox will behave as if records involving these domains do not exist.
                        </preformat>

                        <para>_wildcard_.docker.internal</para>
                        <para>ignoreme.com.au</para>
                        <para>ignoreme.dev</para>
                </fragment>
        </section>
